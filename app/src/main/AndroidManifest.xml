<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teamcarl.prototype">

    <!-- NEEDED TO ACCESS NETWORK/WIFI -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Register">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainMenu"
            android:screenOrientation="portrait" />
        <activity android:name=".Messages" />
        <activity android:name=".NewMessage" android:windowSoftInputMode="stateVisible|adjustPan"/>
        <activity android:name=".fullMessage" />
        <activity android:name=".Guide" />
        <activity android:name=".MainActivity"
            android:screenOrientation="sensorLandscape" />
        <activity android:name=".DayView"
            android:screenOrientation="sensorLandscape" />
        <activity android:name=".SurveyPage"
            android:screenOrientation="sensorLandscape" />
        <activity android:name=".FinalEvaluation" />

        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".login" />
        <activity android:name=".RegisterInfo" />
        <activity
            android:name=".Purpose"
            android:label="@string/title_activity_purpose" />
        <activity
            android:name=".Final_Referal"
            android:label="@string/title_activity_final__referal" />
        <activity
            android:name=".Activemenu"
            android:label="@string/title_activity_activemenu"></activity>
    </application>

</manifest>